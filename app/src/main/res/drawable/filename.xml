<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
   
    <item>
        <shape android:shape="rectangle">
            <stroke 
                android:width="1.2px"
                android:color="#E7EAED"/>
        </shape>
    </item>
     
    <item
        android:left="1.2px"
        android:top="1.2px"
        android:right="1.2px"
        android:bottom="1.2px">
        <shape android:shape="rectangle">
            <solid 
                android:color="#FFFFFF"/>
        </shape>
    </item>
 
</layer-list>

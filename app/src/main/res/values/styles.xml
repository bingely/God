<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppBaseTheme" parent="Theme.AppCompat.Light"></style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
    </style>

    <!-- actionbar start -->
    <style name="actionbar_style">
        <item name="android:background">@color/happyred</item>
        <item name="android:layout_height">@dimen/actionbar_height</item>
        <item name="android:layout_width">match_parent</item>
    </style>


    <style name="actionbar_home_style">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:paddingTop">6dip</item>
        <item name="android:paddingBottom">6dip</item>
        <item name="android:paddingRight">20dip</item>
        <item name="android:paddingLeft">-8dip</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:src">@drawable/actionbar_home</item>
        <item name="android:contentDescription">返回上一层左箭头</item>
    </style>

    <style name="actionbar_title_style">
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textSize">@dimen/txt_20</item>
        <item name="android:textColor">@android:color/white</item>
        <!-- 避免过长标题 -->
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:maxLength">10</item>
    </style>

    <style name="actionbar_action_style">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginRight">@dimen/margin_18</item>
        <item name="android:textSize">@dimen/actionbar_action_textsize</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <!-- actionbar end -->


    <!-- login start -->
    <style name="login_edittext_style">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:singleLine">true</item>
    </style>
    <!-- login end -->


    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~列表 start ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <style name="list_image_style">
        <item name="android:layout_width">@dimen/list_image_size</item>
        <item name="android:layout_height">@dimen/list_image_size</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_marginBottom">@dimen/list_image_margin_top</item>
        <item name="android:layout_marginLeft">@dimen/list_image_margin_left</item>
        <item name="android:layout_marginTop">@dimen/list_image_margin_top</item>
    </style>

    <style name="item_order_tv_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:textColor">#797979</item>
    </style>

    <style name="detail_row_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="detail_tv_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center_vertical|left</item>
    </style>

    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~列表 end ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

    <style name="fragment_title_style">
        <item name="android:layout_height">@dimen/header_height</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@color/happyred</item>
        <item name="android:textSize">@dimen/header_text_size</item>
    </style>

    <!-- 表单风格 start -->
    <style name="form_style">
        <item name="android:background">@android:color/white</item>
    </style>

    <style name="form_divder_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">#dfe3e6</item>
    </style>

    <style name="form_tv_style">
        <item name="android:layout_marginLeft">@dimen/global_margin_left</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/form_item_height</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/form_text_size</item>
    </style>

    <style name="form_other_style">
        <item name="android:layout_marginRight">@dimen/global_margin_right</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/form_item_height</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_weight">2</item>
    </style>
    <!-- 表单风格end -->
    <!-- 收入 start -->
    <style name="income_tv_key">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@android:color/darker_gray</item>
        <item name="android:paddingLeft">7dp</item>
    </style>

    <style name="income_tv_value">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">24sp</item>
        <item name="android:text">00.00</item>
        <item name="android:paddingLeft">7dp</item>
    </style>
    <!-- 收入 end -->
    <style name="order_detail"></style>

    <style name="toggle_iv_style">
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="toggle_tv_style">
        <item name="android:textSize">16sp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="toggle_layout_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="toggle_btn_style">
        <item name="android:layout_width">wrap_content</item>

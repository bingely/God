        <item name="android:textSize">10sp</item>
        <item name="android:background">@drawable/chat_time_bg</item>
        <item name="android:text">2015-10-12</item>
    </style>

    <style name="chat_image_style">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:minHeight">30dp</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="chat_avtar_style">
        <item name="android:layout_width">@dimen/chat_list_item_image</item>
        <item name="android:layout_height">@dimen/chat_list_item_image</item>
        <item name="android:clickable">true</item>
        <item name="android:src">@drawable/loading_default</item>
    </style>

    <!--  -->
    <style name="Theme.Transparent" parent="android:Theme">
        <item name="android:windowIsTranslucent">true</item>

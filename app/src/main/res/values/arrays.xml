   <!--  小票打印指令 -->
        <string-array name="cmd">
        <item>1B 2B</item><!-- 检测型号 -->
        <item>09</item><!-- 水平制表 -->
        <item>0A</item><!-- 换行 -->
        <item>0D</item><!-- 打印当前存储内容 -->
        <item>1B 40</item><!-- 初始化打印机 -->
        <item>1C 2D 01</item><!-- 允许下划线打印 -->
        <item>1C 2D 00</item><!-- 禁止下划线打印 -->
        <item>1B 45 01</item><!-- 允许粗体打印 -->
        <item>1B 45 00</item><!-- 禁止粗体打印 -->
        <item>Select the font: ASCII (12 * 24) characters (24 * 24),1B 4D 00</item>
        <item>Select the font: ASCII (8 * 16) characters (16 * 16),1B 4D 01</item>
        <item>1D 21 00</item><!-- 不放大 -->
        <item>1D 21 01</item><!-- 纵向放大 -->
        <item>1D 21 10</item><!-- 横向放大 -->
        <item>1D 21 11</item><!-- 字符两倍整体放大 -->
        <item>1B 61 00</item><!-- 左对齐 -->
        <item>1B 61 01</item><!-- 居中对齐 -->
        <item>1B 61 02</item><!-- 居右对齐 -->
        <item>0C</item><!--页进纸-->
    </string-array>

